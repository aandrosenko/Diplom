@using WebUI.Models
@model ShopInfoModel


@{
    ViewBag.Title = "Каталог магазинов";
}


<div class="catalog-list">
    @foreach (var shop in Model.ShopInfoes)
    {
        <div>
            <h4>
                <a href="@Url.Action("ShopDescription", "Shop", new { itemId = shop.ShopInfoId })">@shop.Name</a>
            </h4>
        </div>
    }
</div>